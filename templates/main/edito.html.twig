<section id="edito" class="test">
    <div class="container">
    {% for edito in editos %}
        <div class="row">
            <div class="col-md-6 m-auto">
                <img src="/images/img-edito.png" alt="" class="img-fluid">
            </div>
            <div class="col-md-6">
                <h1 class="text-center my-3">{{ edito.title }}</h1>
                <p>{{ edito.content|raw }}</p>
            </div>
        </div>
    </div>
    {% endfor %}
</section>